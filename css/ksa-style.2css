@media(max-width:767px){
.ksa-locationmapping .ksa-mapwrap .ksa-mapcontainer{display:none;}
.ksa-locationmapping .ksa-mapwrap .ksa-mapoverlay{position:relative;top:auto;left:auto;}
.ksa-locationmapping .ksa-mapwrap .ksa-mapoverlay .locationmarker{margin:0 0 5px;position:static;/*position:relative;top:auto !important;left:auto !important;right:auto !important;*/}
